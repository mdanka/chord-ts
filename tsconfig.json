{
    "compilerOptions": {
      "declaration": true,
      "importHelpers": true,
      "lib": [
        "dom",
        "es5"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "outDir": "lib",
      "strict": true,
      "target": "es5"
    },
    "exclude": [
      "test",
      "node_modules",
      "lib"
    ]
  }